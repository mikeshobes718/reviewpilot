option_settings:
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production
    # Note: These environment variables need to be set manually in the Elastic Beanstalk console
    # or through the AWS CLI. The actual values should not be committed to git.
    # STRIPE_SECRET_KEY: (set manually)
    # STRIPE_WEBHOOK_SECRET: (set manually)
    # STRIPE_SUBSCRIPTION_WEBHOOK_SECRET: (set manually)
    # NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY: (set manually)
    # STRIPE_PRO_PRICE_ID: (set manually)
    # POSTMARK_API_KEY: (set manually)
    # POSTMARK_FROM_EMAIL: (set manually)
    # GOOGLE_PLACES_API_KEY: (set manually)
